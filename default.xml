<remote name="firefly-linux" fetch="https://gitlab.com/firefly-linux/yocto" revision="rk3588/firefly"/>
<remote name="firefly-dev" fetch="https://github.com/sgunin" revision="main"/>
	
<project name="meta-openembedded" remote="firefly-linux" path="source/meta-openembedded"/>
<project name="meta-qt5" remote="firefly-linux" path="source/meta-qt5"/>
<project name="meta-rockchip" remote="firefly-linux" path="source/meta-rockchip"/>
<project name="meta-clang" remote="firefly-linux" path="source/meta-clang"/>
<project name="meta-browser" remote="firefly-linux" path="source/meta-browser"/>
<project name="meta-lts-mixins" remote="firefly-linux" path="source/meta-lts-mixins"/>
<project name="poky" remote="firefly-linux" path="source/poky">
 <linkfile dest="README.md" src="build/README_yocto.md"/>
