<?xml version="1.0" encoding="UTF-8"?>
<manifest>
 <notice>Yocto release 5.1 (styhead)</notice>

 <default revision="styhead" sync-j="16" sync-c="true"/>

 <remote name="oe" fetch="https://git.openembedded.org"/>
 <remote name="yocto" fetch="https://git.yoctoproject.org/git"/>
 <remote name="jeffycn" fetch="https://github.com/JeffyCN" revision="master"/>
 <remote name="firefly-dev" fetch="https://github.com/sgunin"/>

 <project name="poky" remote="yocto" path="sources/poky"/>
 <project name="meta-openembedded" remote="oe" path="sources/meta-openembedded"/>
 <project name="meta-clang" remote="clang" path="sources/meta-clang"/>
 <project name="meta-rockchip" remote="jeffycn" path="sources/meta-rockchip"/>
 <project name="meta-firefly-dev" remote="firefly-dev" path="sources/meta-firefly-dev">
  <linkfile src="local.conf" dest="build/conf/local.conf"/>
  <linkfile src="bblayers.conf" dest="build/conf/bblayers.conf"/>
  <linkfile src="setup-environment" dest="setup-environment"/>
 </project>
			      
</manifest>
