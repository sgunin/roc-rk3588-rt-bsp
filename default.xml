<?xml version="1.0" encoding="UTF-8"?>
<manifest>

<default sync-j="4" sync-c="true"/>
	
<remote name="firefly-linux" fetch="https://gitlab.com/firefly-linux" revision="refs/tags/rk3588/linux_yocto_release"/>
<remote name="firefly-dev" fetch="https://github.com/sgunin" rev:wq!ision="main"/>
	
<project name="meta-openembedded" remote="firefly-linux" path="yocto/meta-openembedded" />
<project name="meta-qt5" remote="firefly-linux" path="yocto/meta-qt5" />
<project name="meta-rockchip" remote="firefly-linux" path="yocto/meta-rockchip" />
<project name="meta-clang" remote="firefly-linux" path="yocto/meta-clang" />
<project name="meta-browser" remote="firefly-linux" path="yocto/meta-browser" />
<project name="meta-lts-mixins" remote="firefly-linux" path="yocto/meta-lts-mixins" />
<project name="poky" remote="firefly-linux" path="yocto/poky" >
